            <VBox alignment="CENTER" prefHeight="150.0" prefWidth="232.0">
               <children>
                  <StackPane prefHeight="150.0" prefWidth="200.0">
                     <children>
                        <Circle fill="DODGERBLUE" radius="200.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" style="-fx-fill: #CC1242;">
