                              <Insets bottom="-350.0" top="100.0" />
                           </StackPane.margin>
                        </Circle>
                        <Circle fill="DODGERBLUE" radius="176.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" style="-fx-fill: #EF745B;">
